lass=\"headline text-deep-purple-accent-2\">Flight Lookup</h3>\n        <v-form @submit.prevent=\"lookupFlights\">\n          <v-row>\n            <v-col cols=\"12\" md=\"6\">\n              <v-text-field v-model=\"departureCity\" label=\"Departure City\"></v-text-field>\n            </v-col>\n            <v-col cols=\"12\" md=\"6\">\n              <v-text-field v-model=\"destinationCity\" label=\"Destination City\"></v-text-field>\n            </v-col>\n          </v-row>\n          <v-row>\n            <v-col>\n              <v-btn type=\"submit\" color=\"deep-purple-accent-2\">Lookup Flights</v-btn>\n            </v-col>\n          </v-row>\n        </v-form>\n        <!-- Display flight results here -->\n        <v-list v-if=\"flightResults.length > 0\">\n          <v-subheader>Flight Results</v-subheader>\n          <v-list-item-group>\n            <v-list-item v-for=\"(flight, index) in flightResults\" :key=\"index\">\n              <v-list-item-content>\n                <v-list-item-title>{{ flight.airline }} - {{ flight.price }}</v-list-item-title>\n              </v-list-item-content>\n            </v-list-item>\n          </v-list-item-group>\n        </v-list>\n      </v-col>\n    </v-row>\n  </v-container>\n</template>\n\n<script>\nimport axios from 'axios';\n\nexport default {\n  data() {\n    return {\n      departureCity: '',\n      destinationCity: '',\n      flightResults: [],\n      apiKey: '3311fb347e8b9eb0da3b5d4b57d2baad', // Replace with your actual AviationStack API key\n      apiUrl: 'https://api.aviationstack.com/v1/flights',\n    };\n  },\n  methods: {\n    lookupFlights() {\n      const { departureCity, destinationCity, apiKey, apiUrl } = this;\n\n      // Make a request to the AviationStack API\n      axios.get(apiUrl, {\n        params: {\n          access_key: apiKey,\n          departure_iata: departureCity,\n          arrival_iata: destinationCity,\n        },\n      })\n      .then(response => {\n        // Handle the flight data\n  