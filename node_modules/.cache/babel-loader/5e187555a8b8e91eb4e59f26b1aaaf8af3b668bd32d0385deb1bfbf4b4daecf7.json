{"ast":null,"code":"import { defineComponent } from 'vue';\nexport default defineComponent({\n  data() {\n    return {\n      featuredDestinations: [{\n        name: 'New York, New York',\n        image: require('@/assets/nyc.jpeg'),\n        description: 'Bustling metropolis known for iconic landmarks, diverse neighborhoods, cultural attractions, and vibrant nightlife.'\n      }, {\n        name: 'Seattle, Washington',\n        image: require('@/assets/seattle.jpeg'),\n        description: 'Seattle, the Emerald City, offers a blend of tech innovation, stunning natural beauty, coffee culture, and a thriving arts scene.'\n      }, {\n        name: 'Honolulu, Hawaii',\n        image: require('@/assets/hawaii.jpeg'),\n        description: 'Honolulu, the capital of Hawaii, is a tropical paradise known for its beautiful beaches, rich cultural heritage, and vibrant Pacific cuisine'\n      }\n\n      // Add more featured destinations as needed\n      ],\n\n      specialFeatures: [{\n        icon: 'mdi-star',\n        title: 'Tailored Itineraries',\n        description: 'Crafting unique travel plans based on your interests and preferences.'\n      }, {\n        icon: 'mdi-clock-time-three-outline',\n        title: 'Time Efficiency',\n        description: 'Save time with automated itinerary generation and smart suggestions.'\n      }, {\n        icon: 'mdi-wallet-outline',\n        title: 'Budget-Friendly',\n        description: 'Travel planning that fits your budget without compromising on experiences.'\n      }\n      // Add more features as needed\n      ]\n    };\n  }\n});","map":{"version":3,"names":["defineComponent","data","featuredDestinations","name","image","require","description","specialFeatures","icon","title"],"sources":["/Users/kai/journey-genius-ui/src/views/Home.vue"],"sourcesContent":["<template>\n  <div class=\"home\">\n    <v-container fluid class=\"no-border\">\n      <!-- Top Section -->\n      <v-row align=\"center\" justify=\"center\" class=\"my-3\">\n        <v-col cols=\"12\" md=\"10\" lg=\"8\" class=\"text-center\">\n          <v-card class=\"pa-5\">\n            <h2 class=\"display-1 font-weight-black\">\n              Generate Timeless\n            </h2>\n            <h2 class=\"display-1 font-weight-black\">\n              Vacation Itineraries with\n            </h2>\n            <h2 class=\"display-1 font-weight-black mb-2\">\n              <span class=\"text-deep-purple-accent-2\">Journey Genius</span>\n            </h2>\n            <!-- Additional smaller text -->\n            <h3 class=\"subtitle-1 text-grey-darken-2\">\n              This application serves as your dedicated travel curator, crafting distinctive itineraries\n            </h3>\n            <h3 class=\"subtitle-1 text-grey-darken-2\">\n              tailored to your interests and budget.\n            </h3>\n\n            <!-- \"Start Now\" -->\n            <router-link to='/StartPlanning'>\n              <v-btn rounded=\"lg\" size=\"large\" color=\"deep-purple-accent-2\" class=\"white--text mt-6\">\n                Start Planning\n              </v-btn>\n            </router-link>\n          </v-card>\n        </v-col>\n      </v-row>\n\n\n\n\n\n\n\n\n      <!-- Why Our Application is Special Section -->\n      <v-row align=\"center\" justify=\"center\" class=\"my-3\" style=\"background-color: #f5f5f5;\">\n        <v-col cols=\"12\" md=\"10\" lg=\"8\" class=\"text-center\">\n          <v-card class=\"pa-5\">\n            <h2 class=\"headline mb-3\">\n              What Makes Our Application Special\n            </h2>\n            <p class=\"body-1\">\n              Lorem ipsum dolor sit amet, consectetur adipiscing elit. Sed do eiusmod tempor incididunt ut labore et\n              dolore magna aliqua.\n            </p>\n            <p class=\"body-1\">\n              Ut enim ad minim veniam, quis nostrud exercitation ullamco laboris nisi ut aliquip ex ea commodo consequat.\n            </p>\n          </v-card>\n        </v-col>\n      </v-row>\n\n\n\n\n      <!-- What Makes Our Application Special -->\n      <v-row justify=\"center\" class=\"my-5\">\n        <v-col cols=\"12\">\n          <h2 class=\"headline font-weight-black text-center\">\n            What Makes Our Application Special\n          </h2>\n        </v-col>\n        <v-col v-for=\"(feature, index) in specialFeatures\" :key=\"index\" cols=\"12\" md=\"6\" lg=\"4\">\n          <v-card class=\"pa-3\" elevation=\"10\">\n            <!-- Replace the placeholder content with your feature information -->\n            <v-icon class=\"display-1 mb-2\">{{ feature.icon }}</v-icon>\n            <h3 class=\"subtitle-1 text-center\">{{ feature.title }}</h3>\n            <p class=\"body-1 text-center\">{{ feature.description }}</p>\n          </v-card>\n        </v-col>\n      </v-row>\n\n\n\n\n\n      <!-- Featured Destinations -->\n      <v-row justify=\"center\" class=\"my-5\">\n        <v-col cols=\"12\">\n          <h2 class=\"headline font-weight-black text-center\">\n            Featured Destinations\n          </h2>\n        </v-col>\n        <v-col v-for=\"(destination, index) in featuredDestinations\" :key=\"index\" cols=\"12\" md=\"4\" lg=\"3\">\n          <v-card class=\"pa-3\" elevation=\"10\">\n            <!-- Replace the placeholder content with your destination information -->\n            <h3 class=\"subtitle-1 text-center mt-2\">{{ destination.name }}</h3>\n            <p class=\"body-1 text-center\">{{ destination.description }}</p>\n            <v-img :src=\"destination.image\" alt=\"Destination Image\"></v-img>\n          </v-card>\n        </v-col>\n      </v-row>\n\n\n\n\n\n      <v-row align=\"start\" justify=\"start\" class=\"mt-5\">\n        <v-col>\n          <h1 class=\"headline font-weight-black\">\n            The Most Optimal way to travel\n          </h1>\n        </v-col>\n      </v-row>\n\n      <v-row align=\"center\" justify=\"center\" class=\"my-3 mx-0\" style=\"background-color: #f5f5f5; border-radius: 10px;\">\n        <v-col cols=\"12\" md=\"10\" lg=\"8\">\n          <v-card class=\"pa-4\">\n            <h2 class=\"headline mb-3\">\n              The Only Tool You'll Ever Need!\n            </h2>\n            <p class=\"body-1\">\n              Say goodbye to the stress of planning and hello to personalized recommendations, efficient itineraries, and\n              seamless dining experiences.\n            </p>\n\n            <!-- Features Section -->\n            <v-row justify=\"space-between\" class=\"mt-4\">\n              <v-col cols=\"12\" md=\"5\">\n                <!-- Optimal Route Planning -->\n                <v-card class=\"pa-3 mb-4\" style=\"background-color: #f9f4fa; border-radius: 10px;\">\n                  <v-icon size=\"36\" class=\"accent--text\">mdi-map-marker-path</v-icon>\n                  <h3 class=\"headline mb-2\">Optimal Route Planning</h3>\n                  <p class=\"body-1\">\n                    Our AI algorithms analyze your preferences to craft the most efficient route, saving you time and\n                    effort.\n                  </p>\n                </v-card>\n              </v-col>\n\n              <v-col cols=\"12\" md=\"5\">\n                <!-- Personalize Your Adventure -->\n                <v-card class=\"pa-3 mb-4\" style=\"background-color: #f4f9e4; border-radius: 10px;\">\n                  <v-icon size=\"36\" class=\"accent--text\">mdi-pencil</v-icon>\n                  <h3 class=\"headline mb-2\">Personalize Your Adventure</h3>\n                  <p class=\"body-1\">\n                    Shape your journey by freely adding, editing, or deleting activities from your itinerary.\n                  </p>\n                </v-card>\n\n                <!-- Local Cuisine Recommendations -->\n                <v-card class=\"pa-3 mb-4\" style=\"background-color: #f9ede4; border-radius: 10px;\">\n                  <v-icon size=\"36\" class=\"accent--text\">mdi-food</v-icon>\n                  <h3 class=\"headline mb-2\">Local Cuisine Recommendations</h3>\n                  <p class=\"body-1\">\n                    Discover local cuisines and hidden gems recommended by our AI, tailored to your taste buds.\n                  </p>\n                </v-card>\n              </v-col>\n            </v-row>\n\n          </v-card>\n        </v-col>\n      </v-row>\n    </v-container>\n\n\n\n\n  </div>\n</template>\n\n<script>\nimport { defineComponent } from 'vue';\n\n\nexport default defineComponent({\n  data() {\n    return {\n      featuredDestinations: [\n        {\n          name: 'New York, New York',\n          image: require('@/assets/nyc.jpeg'),\n          description: 'Bustling metropolis known for iconic landmarks, diverse neighborhoods, cultural attractions, and vibrant nightlife.'\n        },\n        {\n          name: 'Seattle, Washington',\n          image: require('@/assets/seattle.jpeg'),\n          description: 'Seattle, the Emerald City, offers a blend of tech innovation, stunning natural beauty, coffee culture, and a thriving arts scene.'\n        },\n        {\n          name: 'Honolulu, Hawaii',\n          image: require('@/assets/hawaii.jpeg'),\n          description: 'Honolulu, the capital of Hawaii, is a tropical paradise known for its beautiful beaches, rich cultural heritage, and vibrant Pacific cuisine'\n        }\n\n        // Add more featured destinations as needed\n      ],\n      specialFeatures: [\n        { icon: 'mdi-star', title: 'Tailored Itineraries', description: 'Crafting unique travel plans based on your interests and preferences.' },\n        { icon: 'mdi-clock-time-three-outline', title: 'Time Efficiency', description: 'Save time with automated itinerary generation and smart suggestions.' },\n        { icon: 'mdi-wallet-outline', title: 'Budget-Friendly', description: 'Travel planning that fits your budget without compromising on experiences.' },\n        // Add more features as needed\n      ],\n    }\n  }\n});\n</script>\n\n<style scoped>\n/* Adjust the font size to make it larger */\n.display-1 {\n  font-size: 4rem;\n  /* You can adjust this value as needed */\n}\n\n/* Add styles for the additional smaller text */\n.subtitle-2 {\n  font-size: 1.5rem;\n  /* Adjust the font size as needed */\n  color: #757575;\n  /* Use a lighter color of black, adjust as needed */\n}\n\n/* Additional styles for the new grid section */\n.headline {\n  font-size: 3rem;\n  font-weight: bold;\n  color: #333;\n  /* Adjust the color as needed */\n}\n\n.body-1 {\n  font-size: 1rem;\n  color: #555;\n  /* Adjust the color as needed */\n}</style>\n"],"mappings":"AA0KA,SAASA,eAAc,QAAS,KAAK;AAGrC,eAAeA,eAAe,CAAC;EAC7BC,IAAIA,CAAA,EAAG;IACL,OAAO;MACLC,oBAAoB,EAAE,CACpB;QACEC,IAAI,EAAE,oBAAoB;QAC1BC,KAAK,EAAEC,OAAO,CAAC,mBAAmB,CAAC;QACnCC,WAAW,EAAE;MACf,CAAC,EACD;QACEH,IAAI,EAAE,qBAAqB;QAC3BC,KAAK,EAAEC,OAAO,CAAC,uBAAuB,CAAC;QACvCC,WAAW,EAAE;MACf,CAAC,EACD;QACEH,IAAI,EAAE,kBAAkB;QACxBC,KAAK,EAAEC,OAAO,CAAC,sBAAsB,CAAC;QACtCC,WAAW,EAAE;MACf;;MAEA;MAAA,CACD;;MACDC,eAAe,EAAE,CACf;QAAEC,IAAI,EAAE,UAAU;QAAEC,KAAK,EAAE,sBAAsB;QAAEH,WAAW,EAAE;MAAwE,CAAC,EACzI;QAAEE,IAAI,EAAE,8BAA8B;QAAEC,KAAK,EAAE,iBAAiB;QAAEH,WAAW,EAAE;MAAuE,CAAC,EACvJ;QAAEE,IAAI,EAAE,oBAAoB;QAAEC,KAAK,EAAE,iBAAiB;QAAEH,WAAW,EAAE;MAA6E;MAClJ;MAAA;IAEJ;EACF;AACF,CAAC,CAAC"},"metadata":{},"sourceType":"module","externalDependencies":[]}